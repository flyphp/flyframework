{
    "name": "flyphp/framework",
    "description": "FlyPHP Framework.",
    "keywords": ["framework", "flyphp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Allan Freitas",
            "email": "allanfreitasci@gmail.com",
            "homepage": "http://allanfreitas.com.br"
        },
        {
            "name": "FlyPHP Community",
            "homepage": "https://github.com/flyphpfw/flyframework/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "classpreloader/classpreloader": "1.0.*",
        "d11wtq/boris": "1.0.*",
        "ircmaxell/password-compat": "1.0.*",
        "filp/whoops": "1.0.10",
        "jeremeamia/superclosure": "1.0.*",
        "monolog/monolog": "1.*",
        "nesbot/carbon": "1.*",
        "patchwork/utf8": "1.1.*",
        "phpseclib/phpseclib": "0.3.*",
        "predis/predis": "0.8.*",
        "stack/builder": "1.0.*",
        "swiftmailer/swiftmailer": "5.0.*",
        "symfony/browser-kit": "2.4.1",
        "symfony/console": "2.4.1",
        "symfony/css-selector": "2.4.1",
        "symfony/debug": "2.4.1",
        "symfony/dom-crawler": "2.4.1",
        "symfony/finder": "2.4.1",
        "symfony/http-foundation": "2.4.1",
        "symfony/http-kernel": "2.4.1",
        "symfony/process": "2.4.1",
        "symfony/routing": "2.4.1",
        "symfony/translation": "2.4.1"
    },
    "require-dev": {
        "aws/aws-sdk-php": "2.5.*",
        "iron-io/iron_mq": "1.4.*",
        "pda/pheanstalk": "2.1.*",
        "mockery/mockery": "0.8.0",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            ["src/Fly/Queue/FlyQueueClosure.php"]
        ],
        "files": [
            "src/Fly/Support/helpers.php"
        ],
        "psr-0": {
            "Fly": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "doctrine/dbal": "Allow renaming columns and dropping SQLite columns."
    },
    "minimum-stability": "dev"
}
